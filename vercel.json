      "NEXT_PUBLIC_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InZ3a3h5ZnVjemRlcXN0bXB1c2tiIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjQwMjc2MjcsImV4cCI6MjA3OTYwMzYyN30.xAVYis2Ap_ZREAt6h23U177JXKkcurE4sWBkwZCFe6U",
      "SUPABASE_SERVICE_ROLE_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InZ3a3h5ZnVjemRlcXN0bXB1c2tiIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImlhdCI6MTc2NDAyNzYyNywiZXhwIjoyMDc5NjAzNjI3fQ.9ylZzxrdU5S5mf_DTFTRHrsOWEly4ZIrQ785791ZDRs",
